<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="jp_btn" parent="android:Widget.Material.Button">
        <item name="android:minWidth">220dp</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/blue_700</item>
        <item name="android:background">@drawable/jp_btn</item>
        <item name="android:onClick">projectClicked</item>
    </style>
    <style name="jp_btn.jp_accent_btn">
        <item name="android:textColor">@color/white</item>
        <item name="android:background">@drawable/jp_accent_btn</item>
    </style>
</resources>